Package: CRiSpData
Title: Data for the City River Spaces packages
Version: 0.0.0.9000
Authors@R: c(
    person("Claudiu", "Forgaci", , "c.forgaci@tudelft.nl", role = c("aut", "cre", "cph"),
           comment = c(ORCID = "0000-0003-3218-5102")),
