Package: CRiSpData
Title: Geospatial Data for Urban River Space Delineation
Version: 0.0.0.9000
Authors@R: c(
    person("Claudiu", "Forgaci", , "c.forgaci@tudelft.nl", role = c("aut", "cre", "cph"),
           comment = c(ORCID = "0000-0003-3218-5102")),
